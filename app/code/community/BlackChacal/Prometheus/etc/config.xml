<config>
    <modules>
        <BlackChacal_Prometheus>
            <version>0.1.0</version>
        </BlackChacal_Prometheus>
    </modules>
    <global>
        <models>
            <prometheus>
                <class>BlackChacal_Prometheus_Model</class>
            </prometheus>
        </models>
        <helpers>
            <prometheus>
                <class>BlackChacal_Prometheus_Helper</class>
            </prometheus>
        </helpers>
    </global>
    <adminhtml>
        <translate>
            <modules>
                <BlackChacal_Prometheus>
                    <files>
                        <default>BlackChacal_Prometheus.csv</default>
                    </files>
                </BlackChacal_Prometheus>
            </modules>
        </translate>
    </adminhtml>
    <default>
        <prometheus>
            <general>
                <enabled>1</enabled>
                <help>1</help>
            </general>
            <default_extension>
                <namespace>BlackChacal</namespace>
                <codepool>community</codepool>
                <version>0.1.0</version>
                <action>archive</action>
                <license><![CDATA[
